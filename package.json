{
  "name": "dbnet-nuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "clean": "rimraf .output .nuxt .vite dist/*",
    "dev": "npm run clean && node scripts/generate-pool-cache.js && node scripts/generate-page-cache.js && nuxt dev",
    "generate": "node scripts/generate-pool-cache.js && node scripts/generate-page-cache.js && nuxt generate",
    "postgenerate": "node scripts/generate-rss.js && node scripts/generate-sitemap.js",
    "preview": "nuxt preview",
    "deploy": "npm run generate && ./scripts/deploy.sh",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "chart.js": "^4.5.1",
    "dateformat": "^5.0.3",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "nuxt": "^4.2.2",
    "parse5": "^8.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "sass-embedded": "^1.97.2",
    "rimraf": "^5.0.0"
  }
}